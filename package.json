    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "nsp": "^3.1.0",
    "standard-version": "^4.3.0"
